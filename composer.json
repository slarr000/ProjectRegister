{
    "name": "slarr000/projectregister",
    "description": "User registration and authentication system with Slim Framework",
    "type": "project",
    "keywords": ["slim", "authentication", "registration", "php"],
    "require": {
        "php": "^8.3",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.0",
      "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "ProjectRegister\\": "src/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
